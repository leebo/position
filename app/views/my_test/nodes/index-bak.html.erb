<h1>Listing my_test_nodes</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @my_test_nodes.each do |my_test_node| %>
      <tr>
        <td><%= my_test_node.name %></td>

        <td><%= link_to 'Show', my_test_node %></td>
        <td><%= link_to 'Edit', edit_my_test_node_path(my_test_node) %></td>
        <td><%= link_to 'Destroy', my_test_node, :confirm => 'Are you sure?', :method => :delete %></td>
      </tr>
  <% end %>
</table>
<ul id="tt" class="easyui-tree">
  <li>
    <span>Folder</span>
    <ul>
      <li>
        <span>Sub Folder 1</span>
        <ul>
          <li>
            <span><a href="#">File 11</a></span>
          </li>
          <li>
            <span>File 12</span>
          </li>
          <li>
            <span>File 13</span>
          </li>
        </ul>
      </li>
      <li>
        <span>File 2</span>
      </li>
      <li>
        <span>File 3</span>
      </li>
    </ul>
  </li>
  <li>
    <span>File21</span>
  </li>
</ul>
<ul id="tt" class="easyui-tree"
    url="my_test/nodes.json">
</ul>

<br/>
<link rel="stylesheet" type="text/css" href="http://www.jeasyui.com/easyui/themes/default/easyui.css">
<link rel="stylesheet" type="text/css" href="http://www.jeasyui.com/easyui/themes/icon.css">
<%= link_to 'New Node', new_my_test_node_path %>
<%= javascript_include_tag "http://www.jeasyui.com/easyui/jquery.easyui.min.js" %>
<script type="text/javascript">
    $('#tree').tree({

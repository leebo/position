<div class="panel panel-default">
  <div class="panel-heading font-bold">配置距离参数</div>
  <div class="panel-body">

    <%= form_tag complete_config_distance_admin_sys_path, multipart: true do %>

        <div class="form-group">
          <label class="col-sm-2 control-label" for="input-id-1">距离</label>

          <div class="col-sm-10">
            <input type="text" class="form-control" name="sys_conf[distance]" id="input-id-1">
          </div>
        </div>

        <div class="form-group">
          <div class="col-lg-offset-2 col-lg-10">
            <button type="submit" class="btn btn-sm btn-info">确定</button>
          </div>
        </div>

    <% end %>
  </div>
</div>



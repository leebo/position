<p id="notice"><%= notice %></p>

<h1>Listing Admin Customers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Mdn</th>
      <th>Emsi</th>
      <th>Imei</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customers.each do |customer| %>
      <tr>
        <td><%= customer.name %></td>
        <td><%= customer.mdn %></td>
        <td><%= customer.emsi %></td>
        <td><%= customer.imei %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

